{
    "name": "rafy-mora/formbuilder-field",
    "description": "add field for create form in your backpackforlaravel admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Raphael Mora",
            "email": "contact@raphael-mora.fr",
            "homepage": "https://raphael-mora.fr"
        }
    ],
    "homepage": "https://github.com/RafyMora/formbuilder-field",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "FormbuilderField"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "backpack/crud": "^4.1.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "RafyMora\\FormbuilderField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RafyMora\\FormbuilderField\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RafyMora\\FormbuilderField\\AddonServiceProvider"
            ]
        }
    }
}
